{
	"name": "jayden-chua/leetcode-notes",
	"description": "PHP Notes for Leetcode Problem Sets",
	"keywords": ["PHP", "Leetcode", "Data Structures", "Algorithms"],
	"require": {
		"php": "^7.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src",
			"Lib\\": "lib"
		}
	},
	"autoload-dev": {
        "psr-4": {
            "App\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "sort-packages": true
    }
}